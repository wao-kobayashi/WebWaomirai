    script.
        //cookieの処理
        $(document).ready(function() {
          var cookieName = 'visited';
          var sessionKey = 'sessionVisited';

          // セッションストレージにデータがあるか確認
          var sessionVisited = sessionStorage.getItem(sessionKey);

          if (!sessionVisited) {
            // セッションストレージにデータが存在しない場合、初回訪問処理を実行
            console.log("初回訪問です！");
            $('.header-speech-tr').show();          
            // セッションストレージにデータを設定（訪問済みとしてマーク）
            sessionStorage.setItem(sessionKey, 'true');
            // クッキーが存在するか確認
            if (!$.cookie(cookieName)) {
              // クッキーが存在しない場合、クッキーを設定（訪問済みとしてマーク）
              $.cookie(cookieName, 'true', { expires: 365 }); // 365日間有効
            }
          } else {
            // セッションストレージにデータが存在する場合の処理
            console.log("再訪問です！");
          }
        });
        $('.header-logo').css({
                'transition': 'all 0.5s ease'  // すべてのプロパティに0.5秒のアニメーションを適用
        });
        function switchToLevel1() {
          $('.content-level-1').show();
          $('.content-level-2').hide();
          $('body').removeClass('level-2').addClass('level-1');
          $('.level-label').text('LEVEL 1');
          
          $('.level-color').css({
          'background': '#005C3F'
          });
          $('body').css({
          'background': '#E9F4F0'
          });
         }
        function switchToLevel2() {
          $('.content-level-1').hide();
          $('.content-level-2').show();
          $('body').removeClass('level-1').addClass('level-2');
          $('.level-label').text('LEVEL 2');
          $('.level-color').css({
              'background': '#AA68AA'
           });
          $('body').css({
          'background': '#EAE4EA'
          });
         }
        $('.level-1').on('click', function() {
          $('.header-speech-tr').fadeOut();
          switchToLevel1();
        });
        $('.level-2').on('click', function() {
          $('.header-speech-tr').fadeOut();
          switchToLevel2();
        });
        function executeFunctionBasedOnURL() {
            // 現在のURLからクエリパラメータを取得
            const urlParams = new URLSearchParams(window.location.search);
            
            // 'via'パラメータが'level-2'か確認
            if (urlParams.get('via') === 'level-2') {
                // 条件が一致した場合に関数を実行
                switchToLevel2();
            }
        }

        // ページが読み込まれたときに実行
        $(document).ready(function() {
            executeFunctionBasedOnURL();
        });
    script. 
      $(window).scroll(function() {
        if ($(window).width() >= 768) {
        // スクロール量を取得
        var scroll = $(window).scrollTop();
        // 200px以上スクロールしたらロゴのサイズとマージンを変更
        if (scroll >= 100) {
          $('.header-logo').css({
            'width': '180px',
            'padding-top': '15px',
            'padding-bottom': '0px'
            
          });
        }  
        if (scroll >= 300) {
          $('.header-speech-tr').fadeOut();
        }
         else {
          // 200px未満の場合は元のサイズに戻す
          $('.header-logo').css({
            'width': '220px',
            'padding-top': '22px',
            'padding-bottom': '0px'
          });
          }
        }
      });
